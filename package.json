{
    "name": "vimdb",
    "version": "3.6.1",
    "description": "Yet another IMDb Scraper",
    "author": "Vinicius Egidio <me@vinicius.io>",
    "license": "ISC",
    "main": "dist/vimdb.js",
    "types": "dist/vimdb.d.ts",
    "targets": {
        "main": {
            "context": "node",
            "includeNodeModules": true
        }
    },
    "devDependencies": {
        "@parcel/packager-ts": "2.0.0-beta.2",
        "@parcel/transformer-typescript-tsc": "2.0.0-beta.2",
        "@parcel/transformer-typescript-types": "2.0.0-beta.2",
        "@types/cheerio": "0.22.29",
        "@types/html-entities": "1.3.3",
        "@types/jest": "26.0.23",
        "@types/node": "15.6.1",
        "@types/node-fetch": "2.5.10",
        "@typescript-eslint/eslint-plugin": "4.26.0",
        "@typescript-eslint/parser": "4.26.0",
        "await-to-js": "^3.0.0",
        "cheerio": "1.0.0-rc.9",
        "eslint": "7.27.0",
        "eslint-plugin-prettier": "^3.4.0",
        "file-type": "16.5.0",
        "html-entities": "2.3.2",
        "jest": "27.0.3",
        "node-fetch": "2.6.1",
        "parcel": "^2.0.0-beta.2",
        "prettier": "^2.3.0",
        "ts-jest": "27.0.2",
        "ts-node": "10.0.0",
        "typedoc": "^0.20.36",
        "typescript": "4.3.2"
    },
    "scripts": {
        "docs": "typedoc --options typedoc.json --disableSources",
        "build": "parcel build src/index.ts --no-source-maps --no-scope-hoist",
        "lint": "eslint . --fix --ext .ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vegidio/js-vimdb"
    },
    "keywords": [
        "imdb",
        "typescript",
        "scraper",
        "movies",
        "series"
    ]
}
